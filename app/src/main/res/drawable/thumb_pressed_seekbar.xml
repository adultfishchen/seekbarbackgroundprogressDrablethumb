<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape
        android:innerRadius="0dp"
        android:shape="ring"
        android:thickness="14dp"
        android:useLevel="false" >
        <size
            android:height="32px"
            android:width="32px"/>
        <gradient
            android:centerColor="#77FF00"
            android:endColor="#CCFF99"
            android:startColor="#00FF00"
            android:type="sweep"
            android:useLevel="false"/>
        <stroke
            android:width="2dp"
            android:color="#FF0088"/>
    </shape>
</item>
    <item>
        <shape
            android:innerRadius="0dp"
            android:shape="ring"
            android:thickness="10dp"
            android:useLevel="false">
            <size
                android:height="32px"
                android:width="32px"/>
            <gradient
                android:centerColor="#0066FF"
                android:centerY="1"
                android:endColor="#BBFFEE"
                android:gradientRadius="20"
                android:startColor="#0000CC"
                android:type="radial"
                android:useLevel="false"/>
            <stroke
                android:width="1dp"
                android:color="#CCCCFF"/>
        </shape>
    </item>
</layer-list>